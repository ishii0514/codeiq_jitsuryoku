    "def bit_count(n, m):\n",
    "    count = 0\n",
    "    for i in range(n+1):\n",
    "        if count_one(i) == m:\n",
    "            count += 1\n",
    "    return count\n",
    "\n",
    "def count_one(n):\n",
    "    return bin(n).count('1')\n",
    "\n",
    "data = input().split()\n",
    "n = int(data[0])\n",
    "m = int(data[1])\n",
    "\n",
    "print(bit_count(n,m))"
   ]
